<view class="content"><block wx:if="{{banner}}"><uni-swiper-dot vue-id="1" info="{{info}}" current="{{current}}" mode="{{mode}}" dots-styles="{{dotsStyles}}" field="content" bind:__l="__l" vue-slots="{{['default']}}"><swiper data-event-opts="{{[['change',[['change',['$event']]]]]}}" class="swiper-box" bindchange="__e"><block wx:for="{{info}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item><view class="swiper-item"><image src="{{item}}"></image></view></swiper-item></block></swiper></uni-swiper-dot></block><view data-event-opts="{{[['tap',[['map',['$event']]]]]}}" class="detail-coordinates" bindtap="__e"><image src="../../static/other/more.png" mode></image><image src="../../static/other/details_address.png" mode></image><view style="padding:1rem;padding-bottom:0.5rem;"><label style="padding-right:1rem;" class="_span">{{list.communityName}}</label><block wx:if="{{list.tenancyFlag=='1'}}"><view class="detail-state">在租</view></block><block wx:else><view class="dated">已到期</view></block></view><view class="detail-date _p">{{list.startDate+" 至 "+list.endDate}}</view><view class="empty"></view></view><view><view class="detail-address"><image src="../../static/other/address_details_icon.png" mode></image><label class="_span">{{list.buildingNumber+"栋"+list.unitNumber+"单元"+list.houseNumber+(list.roomNumber?'-'+list.roomNumber:'')}}</label><label class="_span">{{list.bedroom+"室"+list.livingRoom+"厅"+list.kitchen+"厨"+list.bathroom+"卫"+list.balcony+"阳台"}}</label></view><view class="empty"></view></view><view><block wx:for="{{contentData}}" wx:for-item="item" wx:for-index="index" wx:key="item"><view class="detail-content"><view class="content-left">{{item}}</view><view class="content-right">{{contentDatas[index]}}</view></view></block></view><view class="gray"></view><view class="detail-configuration"><view><view class="configuration">房屋配置</view><view class="configuration-content">{{houseConfiguration}}</view></view></view><view class="gray"></view><view class="detail-houseUser"><view class="_p">房东</view><view data-event-opts="{{[['tap',[['landlordDetail',['$0','$1'],['list.uid','list.id']]]]]}}" class="houseUser" bindtap="__e"><image src="../../static/other/landlord_head.png" mode></image><view class="landlordname">{{list.truename}}</view></view></view><view class="gray"></view><view class="detail-pretend"><view class="_p">{{"租友（"+pretend.length+"）"}}</view><block wx:for="{{pretend}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['friendDetail',['$0','$1','$2'],[[['pretend','',index,'tenancyFlag']],[['pretend','',index,'tenantId']],[['pretend','',index,'sex']]]]]]]}}" class="pretend" bindtap="__e"><block wx:if="{{item.tenancyFlag==1}}"><image class="pretend-img" src="{{item.photo}}" mode></image></block><block wx:else><image class="pretend-img" src="../../static/other/pretendfri_girl_gary.png" mode></image></block><view class="pretend-name">{{item.tenant}}</view></view></block></view><view class="gray"></view><block wx:if="{{rentobj.tenancyTerm}}"><view class="detail-contract"><label class="_span">{{rentobj.tenancyTerm}}</label><navigator href data-event-opts="{{[['tap',[['hetongdetail',['$0',0],['rentobj.contractId']]]]]}}" bindtap="__e" class="_a">查看合同详情<label class="rightIcon _span"></label></navigator></view></block><block wx:else><view class="detail-contract"><label class="_span">尚未添加合同</label></view></block><view class="gray"></view><block wx:if="{{rentobj.fixedCosts}}"><view class="detail-contract"><label class="_span">{{"月租金"+rentobj.fixedCosts+"元"}}</label><navigator href data-event-opts="{{[['tap',[['rentdetail',['$0'],['tenancyNum']]]]]}}" bindtap="__e" class="_a">更多租赁信息<label class="rightIcon _span"></label></navigator></view></block><block wx:else><view class="detail-contract"><label class="_span">尚未设置租金</label></view></block></view>