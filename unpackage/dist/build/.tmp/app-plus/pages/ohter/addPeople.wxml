<view><view class="person-list"><cmd-cell-item vue-id="1" title="关系" addon="{{addon}}" slot-left arrow data-event-opts="{{[['^click',[['goDetailPage']]]]}}" bind:click="__e" bind:__l="__l"></cmd-cell-item></view><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="people-information"><view class="common-left"><text class="common-text">{{item.name}}</text></view><view class="common-right"><block wx:if="{{item.type==1}}"><input type="text" maxlength="{{item.maxlength}}" placeholder="{{item.plac}}" data-event-opts="{{[['input',[['__set_model',['$0',index,'$event',[]],['data']]]]]}}" value="{{data[index]}}" bindinput="__e"/></block><block wx:else><input type="number" maxlength="{{item.maxlength}}" placeholder="{{item.plac}}" data-event-opts="{{[['input',[['__set_model',['$0',index,'$event',[]],['data']]]]]}}" value="{{data[index]}}" bindinput="__e"/></block></view></view></block><view class="picture"><text class="picture-title">身份证照片（非必填）</text><image class="upload-pos" src="{{pic1}}" mode data-event-opts="{{[['tap',[['chooseImg',[0]]]]]}}" catchtap="__e"></image><image class="upload-opp" src="{{pic2}}" mode data-event-opts="{{[['tap',[['chooseImg',[1]]]]]}}" catchtap="__e"></image></view><block wx:if="{{tabbar}}"><button data-event-opts="{{[['tap',[['submit',['$event']]]]]}}" style="background:#2CC3BE;color:#fff;" catchtap="__e">{{button}}</button></block></view>