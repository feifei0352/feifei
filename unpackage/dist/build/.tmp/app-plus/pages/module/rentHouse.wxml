<view><scroll-view class="top" scroll-x><block wx:for="{{tabBar}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['clickTab',[index]]]]]}}" class="{{['contact-tabbar',(currentTab==index)?'tabbar-bottom':'']}}" bindtap="__e"><label class="_span">{{item.title}}</label></view></block></scroll-view><swiper current="{{currentTab}}" data-event-opts="{{[['change',[['changeTab',['$event']]]]]}}" bindchange="__e"><swiper-item><view><block wx:for="{{List}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="contact-item"><view class="contact-item-area"><image src="../../static/other/address_orangepoint.png"></image><text>{{item.street}}</text><label style="color:#2FC38D;" class="_span">{{item.rentWay==1?'单间':'整租'}}</label></view><view data-event-opts="{{[['tap',[['toDetail',['$0','$1'],[[['List','',index,'id']],[['List','',index,'tenancyNum']]]]]]]}}" class="contact-item-year" bindtap="__e"><view class="contact-item-y"><view class="_p">{{item.communityName}}</view><view class="_p">{{item.buildingNumber+"栋"+item.unitNumber+"单元"+item.houseNumber+(item.roomNumber?'-'+item.roomNumber:'')}}</view></view><label class="contact-item-right _span">{{''+item.bedroom+"室"+item.livingRoom+"厅"+item.kitchen+"厨"+item.bathroom+"卫"+item.balcony+'阳台'}}<uni-icon vue-id="{{'1-'+index}}" type="forward" color="#ddd" size="20" bind:__l="__l"></uni-icon></label></view></view></block></view></swiper-item><swiper-item><view class="_ul"><block wx:for="{{expire}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="contact-item _li"><view class="contact-item-area"><image src="../../static/other/address_orangepoint(1).png"></image><text>{{item.street}}</text><label class="_span">{{item.rentWay==1?'单间':'整租'}}</label></view><view data-event-opts="{{[['tap',[['toDetail',['$0','$1'],[[['expire','',index,'id']],[['expire','',index,'tenancyNum']]]]]]]}}" class="contact-item-year" bindtap="__e"><view class="contact-item-y"><view class="_p">{{item.communityName}}</view><view class="_p">{{item.buildingNumber+"栋"+item.unitNumber+"单元"+item.houseNumber}}</view></view><label class="contact-item-right _span">{{''+item.bedroom+"室"+item.livingRoom+"厅"+item.kitchen+"厨"+item.bathroom+"卫"+item.balcony+'阳台'}}<uni-icon vue-id="{{'2-'+index}}" type="forward" color="#ddd" size="20" bind:__l="__l"></uni-icon></label></view></view></block></view><block wx:if="{{expire.length=='0'}}"><view class="none"><view><image src="../../static/other/renants_message.png"></image></view><view>暂无到期租房</view></view></block></swiper-item></swiper></view>