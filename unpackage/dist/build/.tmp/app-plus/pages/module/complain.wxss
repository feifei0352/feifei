
.complain-contain {
	padding: 40rpx 36rpx;
	font-size: 32rpx;
	color: #b5b5b5;
}
.complain-title {
	color: #333333;
	margin-bottom: 40rpx;
	margin-top: 40rpx;
}
.complain-reasons>view {
	border: 1px solid rgba(194, 194, 194, 1);
	height: 88rpx;
	line-height: 88rpx;
	border-radius: 4px;
	text-align: center;
	margin-bottom: 20rpx;
}
.complain-morereasons {
	border: 1px solid rgba(194, 194, 194, 1);
	border-radius: 4px;
	margin-top: 70rpx;
}
.wordwrap {
	float: right;
	margin: 270rpx 8rpx 0 0;
}
.checked {
	background: #fff8ed;
	color: #f2a43d;
	border: 1px solid rgba(252, 174, 71, 1);
}

