	background-color: #ffffff;
	margin-bottom: 20rpx;
}
.wallet-balance {
	line-height: 374rpx;
}
.contractDetailBottom {
	height: 132rpx;
}
.behaviour {
	width: 100%;
	height: 132rpx;
	line-height: 132rpx;
	position: fixed;
	bottom: 0;
	left: 0;
	background: #fff;
	text-align: center;
	border-top: 1rpx solid rgba(225, 230, 229, 1);
}
.behaviour > ._span {
	display: inline-block;
	width: 324rpx;
	height: 88rpx;
	-webkit-border-radius: 4rpx;
	        border-radius: 4rpx;
	text-align: center;
	line-height: 88rpx;
	font-size: 32rpx;
	color: #fff;
	margin: 0 15rpx;
}
.behaviour > ._span:nth-child(1) {
	background: #c8e6e5;
	color: #099c99;
}
.behaviour > ._span:nth-child(2) {
	background: #2cc3be;
}

