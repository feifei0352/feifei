<view><view class="uni-padding-wrap uni-common-mt"><scroll-view class="top" scroll-x><block wx:for="{{tabBar}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['clickTab',[index]]]]]}}" class="{{['contact-tabbar _div',(currentTab==index)?'tabbar-bottom':'']}}" bindtap="__e"><label class="_span">{{item.title}}</label></view></block></scroll-view><swiper style="{{('height:'+swiperHeight)}}" current="{{currentTab}}" data-event-opts="{{[['change',[['changeTab',['$event']]]]]}}" bindchange="__e"><swiper-item><view class="_ul"><block wx:for="{{now}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['detail',['$0','$1'],[[['now','',index,'id']],[['now','',index,'type']]]]]]]}}" class="list _li" bindtap="__e"><image src="{{item.avatarUrl}}" class="_img"></image><uni-badge vue-id="{{'1-'+index}}" text="1" type="error" bind:__l="__l"></uni-badge><label class="_span">{{item.name}}</label><view class="_p">{{item.address}}</view></view></block></view></swiper-item><swiper-item><view class="_ul"><block wx:for="{{history}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['detail',['$0','$1'],[[['history','',index,'id']],[['history','',index,'type']]]]]]]}}" class="list _li" bindtap="__e"><image src="{{item.avatarUrl}}" class="_img"></image><label class="_span">{{item.name}}</label><view class="_p">{{item.address}}</view></view></block></view></swiper-item></swiper></view></view>