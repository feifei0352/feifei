page {
  background: #f2f2f2;
}
.top {
  width: 100%;
  text-align: center;
  line-height: 42px;
  white-space: nowrap;
  position: relative;
  background: #fff;
}
.contact-tabbar {
  width: 50%;
  font-size: 18px;
  height: 42px;
  display: inline-block;
  color: #000;
}
.tabbar-bottom {
  color: #22c7c7;
}
.tabbar-bottom ._span {
  border-bottom: 4px solid #22c7c7;
  padding-bottom: 6px;
}
.contact-item {
  background: #fff;
  margin: 30rpx 0;
}
.contact :active {
  background: #E9EDF0;
}
swiper,
swiper-item {
  height: 900rpx;
  overflow: scroll;
}
.contact-item-area {
  font-size: 32rpx;
  font-family: PingFang SC;
  font-weight: 600;
  color: #333333;
  padding: 36rpx 39rpx;
  border-bottom: 1px solid #ebebeb;
}
.contact-item-area > image {
  width: 32rpx;
  height: 32rpx;
  margin-right: 18rpx;
  float: left;
}
.contact-item-area ._span {
  float: right;
  font-weight: 500;
}
.contact-item-year {
  padding: 36rpx 39rpx;
}
.contact-item-year > image {
  width: 36rpx;
  height: 36rpx;
  margin-top: 5rpx;
  vertical-align: top;
  margin-right: 10rpx;
}
.contact-item-y {
  font-size: 30rpx;
  color: #333333;
  margin-bottom: 10rpx;
  display: inline-block;
}
.contact-item-y > ._p {
  font-size: 32rpx;
  color: #333333;
  line-height: 40rpx;
}
.contact-item-y .roomPosition {
  color: #999999;
}
.none {
  text-align: center;
  font-size: 30rpx;
  color: #333333;
  margin-top: 224rpx;
}
.none image {
  width: 169rpx;
  height: 169rpx;
}
.contact-item-right {
  float: right;
  font-size: 28rpx;
  margin-top: 20rpx;
}
text {
  display: inline-block;
  width: 80%;
  word-wrap: break-word;
}
.add-button {
  margin-top: 56rpx;
  color: #0DBFBC;
  font-size: 34rpx;
  width: 690rpx;
  height: 100rpx;
  line-height: 100rpx;
  border: 2rpx solid #0DBFBC;
  border-radius: 48rpx;
}
.add-button > image {
  width: 34rpx;
  height: 34rpx;
  vertical-align: middle;
  margin-bottom: 8rpx;
  margin-right: 12rpx;
}
.active {
  background: #E9EDF0;
}
.shade {
  position: fixed;
  z-index: 100;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  -webkit-touch-callout: none;
}
.shade .pop {
    position: fixed;
    z-index: 101;
    width: 200rpx;
    -webkit-box-sizing: border-box;
            box-sizing: border-box;
    font-size: 28rpx;
    text-align: left;
    color: #333;
    background-color: #fff;
    -webkit-box-shadow: 0 0 5px rgba(0, 0, 0, 0.5);
            box-shadow: 0 0 5px rgba(0, 0, 0, 0.5);
    line-height: 80rpx;
    -webkit-transition: -webkit-transform 0.15s ease-in-out 0s;
    transition: -webkit-transform 0.15s ease-in-out 0s;
    -o-transition: transform 0.15s ease-in-out 0s;
    transition: transform 0.15s ease-in-out 0s;
    transition: transform 0.15s ease-in-out 0s, -webkit-transform 0.15s ease-in-out 0s;
    -webkit-user-select: none;
       -moz-user-select: none;
        -ms-user-select: none;
            user-select: none;
    -webkit-touch-callout: none;
    -webkit-transform: scale(0, 0);
        -ms-transform: scale(0, 0);
            transform: scale(0, 0);
}
.shade .pop.show {
      -webkit-transform: scale(1, 1);
          -ms-transform: scale(1, 1);
              transform: scale(1, 1);
}
.shade .pop > view {
      padding: 0 20rpx;
      overflow: hidden;
      -o-text-overflow: ellipsis;
         text-overflow: ellipsis;
      white-space: nowrap;
      -webkit-user-select: none;
         -moz-user-select: none;
          -ms-user-select: none;
              user-select: none;
      -webkit-touch-callout: none;
}
.shade .pop > view:active {
        background-color: #f3f3f3;
}

