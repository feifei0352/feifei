<view><scroll-view class="top" scroll-x><block wx:for="{{tabBar}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['clickTab',[index]]]]]}}" class="{{['contact-tabbar',(currentTab==index)?'tabbar-bottom':'']}}" bindtap="__e"><label class="_span">{{item.title}}</label></view></block></scroll-view><swiper current="{{currentTab}}" data-event-opts="{{[['change',[['changeTab',['$event']]]]]}}" bindchange="__e"><swiper-item><view class="contact"><block wx:for="{{List}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['longpress',[['handles',['$event']]]],['tap',[['houseDetail',['$event']]]]]}}" class="contact-item" bindlongpress="__e" bindtap="__e"><view class="contact-item-area"><image src="../../../static/other/address_orangepoint.png"></image><text>{{item.street}}</text><label style="color:#2FC38D;" class="_span">{{item.rentWay==1?'单间':'整租'}}</label></view><view class="contact-item-year"><image src="../../../static/icon/pipeifangdong@3x.png"></image><view class="contact-item-y"><view class="_p">{{item.communityName}}</view><view class="roomPosition _p">{{item.buildingNumber+"栋"+item.unitNumber+"单元"+item.houseNumber+(item.roomNumber?'-'+item.roomNumber:'')}}</view></view><label class="contact-item-right _span"><uni-icon vue-id="{{'1-'+index}}" type="forward" color="#ddd" size="24" bind:__l="__l"></uni-icon></label></view></view></block></view></swiper-item><swiper-item><view class="_ul"><block wx:for="{{expire}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['longpress',[['handles',['$event']]]],['tap',[['houseDetail',['$event']]]]]}}" class="contact-item _li" bindlongpress="__e" bindtap="__e"><view class="contact-item-area"><image src="../../../static/other/address_orangepoint(1).png"></image><text>{{item.street}}</text><label class="_span">{{item.rentWay==1?'单间':'整租'}}</label></view><view class="contact-item-year"><cover-image src="../../../static/icon/jio.png"></cover-image><view class="contact-item-y"><view class="_p">{{item.communityName}}</view><view class="_p">{{item.buildingNumber+"栋"+item.unitNumber+"单元"+item.houseNumber}}</view></view><label class="contact-item-right _span"><uni-icon vue-id="{{'2-'+index}}" type="forward" color="#ddd" size="20" bind:__l="__l"></uni-icon></label></view></view></block></view><block wx:if="{{expire.length=='0'}}"><view class="none"><view><image src="../../../static/icon/renants_jio.png"></image></view><view>暂无到期租房</view></view></block></swiper-item></swiper><button data-event-opts="{{[['tap',[['addHouse',['$event']]]]]}}" class="add-button" bindtap="__e"><image src="../../../static/icon/jio@3x.png"></image>添加我的租房</button><view data-event-opts="{{[['tap',[['hidePop',['$event']]]]]}}" hidden="{{!(showShade)}}" class="shade" bindtap="__e"><view class="{{['pop',(showPop)?'show':'']}}" style="{{(popStyle)}}"><block wx:for="{{popButton}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-index="{{index}}" data-event-opts="{{[['tap',[['pickerMenu',['$0'],[[['popButton','',index]]]]]]]}}" bindtap="__e">{{item}}</view></block></view></view></view>