
.house-name{
		font-size:40rpx;
		font-weight:bold;
		display:inline-block;
		width:504rpx;
		line-height:48rpx;
}
.location{
		width:76rpx;
		height:76rpx;
		vertical-align: top;
		margin-left:96rpx;
}
.house-date{
		margin:24rpx 0;
}
.house-date>text{
		font-size:24rpx;
		color:#5D5D5D;
}
.house-tags{
		margin:5rpx 0 26rpx 0;
}
.house-tags>button{
		display:inline-block;
		background:#DCF7F6;
		font-size:22rpx;
		height:42rpx;
		line-height:42rpx;
		color:rgba(7,181,169,1);
		margin-right:12rpx;
}
.house-info{
		display:-webkit-box;
		display:-webkit-flex;
		display:-ms-flexbox;
		display:flex;
}
.house-info>view{
		position:relative;
		text-align: center;
}
.house-info>view>text{
		display:block;
}
.house-info>view:after{
		position:absolute;
		top:10rpx;
		height: calc(100% - 20rpx); 
		left:0;
		content:'';
		width:0;
		border-left:1rpx solid rgb(240,240,240);
}
.house-info>view:first-child:after{
		border:none;
}
.info-1{
		font-size:30rpx;
		color:#F78640;
		margin-top:12rpx;
}
.info-2{
		font-size:22rpx;
		color:#BDBDBD;
		margin-top:20rpx;
}
.house-configuration:after{
		content:'';
		display:block;
		clear: both;
}
.config-title{
		color:#333333;
		font-weight:bold;
		font-size:30rpx;
		margin:36rpx 0 26rpx 0;
}
.config-item{
		float:left;
		width:20%;
		text-align: center;
}
.config-item>image{
		display:block;
		margin:0 auto;
		width:100rpx;
		height:100rpx;
}
.config-item>text{
		color:#333333;
		font-size:22rpx;
}
.house-context{
		font-size:#333333;
		font-size:26rpx;
		line-height: 36rpx;
}
.house-context>text{
		display:block;
		padding-bottom:24rpx;
		border-bottom:1rpx solid rgb(240,240,240);
}
.house-context>image{
		display:block;
		width:82rpx;
		height:62rpx;
		margin:0 auto
}
.renters{
	    overflow:scroll;
		height:240rpx;
		display:-webkit-box;
		display:-webkit-flex;
		display:-ms-flexbox;
		display:flex;
}
.renter{
		margin-right:19rpx;
}
.renter>view{
		background:rgb(250,250,250);
		width:220rpx;
		height:240rpx;
}
.renter>view>image{
		display:block;
		width:120rpx;
		height:120rpx;
		margin:0 auto
}
.renter>view>text{
		display:block;
		text-align: center;
		color:#333333;
		font-size:26rpx;
		height:60rpx;
		line-height:62rpx;
}
.renter>view>text:last-child{
		font-size:22rpx;
}
.contract{
		height:62rpx;
		line-height:62rpx;
		background:rgba(235,248,255,1);
		border:1rpx solid rgba(102,199,255,1);
		border-radius:12rpx;
		color:#0096ED;
		padding:32rpx 28rpx 31rpx 45rpx;
		font-size:30rpx;
		margin-top:31rpx;
		margin-bottom:20rpx
}
.contract-detail{
		display:inline-block;
		float:right;
}
.contract-detail>image{
		width:42rpx;
		height:42rpx;
		vertical-align: middle;
}
.house-touch>button{
		width:338rpx;
		height:88rpx;
		line-height:88rpx;
		border-radius:4rpx;
		color:#fff;
		font-size:32rpx;
		display:inline-block;
		margin-top:22rpx
}
.online{
		background:#11D6D3;
		margin-right:24rpx;
}
.phone{
		background:#F5CB11
}
.house-touch{
		height:128rpx;
}
.container{
		height:1064rpx;
		overflow: scroll;
}
.pop{
		width:100%;
		background:#fff;
		position:fixed;
		z-index:101;
		bottom:0;
		padding-top:30rpx;
		left:0;
		border-radius:48rpx 48rpx 0 0;
}
.pop>.config-title{
		margin-left:30rpx
}
.pop .config-item{
		background:#fff;
        width:20%;
		margin:20rpx 0;
}
.bg{
		position: absolute;
		background-color: rgba(0,0,0,0.5);
		z-index: 100;
		width: 100%;
		height: 100%;
		left: 0;
		top: 0;
}

