<view><view class="friendDetail-basic"><block wx:if="{{type==0}}"><view class="datedSymbol">已到期</view></block><view class="friendDetail-photo"><image style="{{(grayPhoto)}}" src="{{myphoto}}"></image></view><view class="friendDetail-name">{{''+user.tenant+''}}<image src="{{mysexImg}}"></image></view><view class="friendDetail-area">{{user.area}}</view></view><view class="friendDetail-other"><view class="friendDetail-room"><text class="friendDetail-title">房间号</text><text>{{user.roomNumber+"号"}}</text></view><view data-event-opts="{{[['tap',[['showPhone',['$event']]]]]}}" class="friendDetail-phone" bindtap="__e"><text class="friendDetail-title">手机号</text><text>{{$root.f0}}</text><image src="../../static/other/call.png"></image></view></view><block wx:if="{{isNow}}"><view class="friendDetail-complaint"><view data-event-opts="{{[['tap',[['complain',['$0','$1'],['id','user.uid']]]]]}}" bindtap="__e">投诉TA</view></view></block><uni-popup vue-id="1" show="{{showPopup}}" position="bottom" bind:__l="__l" vue-slots="{{['default']}}"><view>拨打租友电话</view><button data-event-opts="{{[['tap',[['call',['$0'],['user.tenantPhone']]]]]}}" style="width:80%;color:#fff;background:rgba(102, 204, 204, 1);margin-bottom:32rpx;" bindtap="__e">{{$root.f1}}</button><button data-event-opts="{{[['tap',[['close',['$event']]]]]}}" style="width:80%;color:rgba(102, 204, 204, 1);border:2rpx solid rgba(102,204,204,1);margin-bottom:60rpx;" bindtap="__e">取消</button></uni-popup></view>