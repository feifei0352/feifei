<view><view class="forms"><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="people-information"><view class="common-left"><text class="common-text">{{item.name}}</text></view><view class="common-right"><block wx:if="{{item.type==0}}"><input type="text" placeholder="{{item.plac}}" data-event-opts="{{[['blur',[['check',['$0'],[[['list','',index,'type']]]]]],['input',[['__set_model',['$0',index,'$event',[]],['data']],['changeColor']]]]}}" value="{{data[index]}}" bindblur="__e" bindinput="__e"/></block><block wx:if="{{item.type==1}}"><input type="number" placeholder="{{item.plac}}" data-event-opts="{{[['blur',[['check',['$0'],[[['list','',index,'type']]]]]],['input',[['__set_model',['$0',index,'$event',[]],['data']],['changeColor']]]]}}" value="{{data[index]}}" bindblur="__e" bindinput="__e"/></block><block wx:else><text style="line-height:60rpx;">{{item.backName}}</text></block></view></view></block></view><view class="submit"><button data-event-opts="{{[['tap',[['submit']]]]}}" style="{{'background:'+(background)+';'}}" bindtap="__e">绑定</button></view></view>